    "ng2",
    "angularjs",
    "table",
    "tableView",
    "grid",
    "filter",
    "sort"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dennis-b/NG2TableView"
  },
  "engines": {
    "node": ">= 4.2.1",
    "npm": ">= 3"
  }
}
